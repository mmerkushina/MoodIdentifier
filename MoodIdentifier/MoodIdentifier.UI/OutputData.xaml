<Window x:Name="OutputDataWindow" x:Class="MoodIdentifier.UI.OutputData"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MoodIdentifier.UI"
        mc:Ignorable="d"
        Title="OutputData" Height="480" Width="640.8">
    <Window.Background>
        <ImageBrush ImageSource="Pictures/background.png"/>
    </Window.Background>

    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="10*"/>
            <RowDefinition Height="1*"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="9*"/>
            <ColumnDefinition Width="2*"/>
        </Grid.ColumnDefinitions>
        <Button x:Name="Button_Back_To_MainWindow" Content="Back" HorizontalAlignment="Left" VerticalAlignment="Top" Width="105" Grid.Column="1" Grid.Row="1" Height="27" Margin="0,4,0,0" Click="Button_Back_To_MainWindow_Click"/>
    </Grid>
</Window>
